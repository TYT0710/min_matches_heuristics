from lib.pyomo_models.milp_transhipment import *
from lib.pyomo_models.milp_transportation import *

def solve_milp(solver,model,instance):
	
	if model=='transhipment':
		solution=solve_milp_transhipment_model(instance,solver)
	if model=='transportation':
		solution=solve_milp_transportation_model(instance,solver)
	return solution